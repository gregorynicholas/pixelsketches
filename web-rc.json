{
    "apiVersion": "v1",
    "kind": "ReplicationController",
    "metadata": {
        "name": "web-v1-4"
    },
    "spec": {
        "replicas": 3,
        "selector": {
            "name": "web",
            "version": "1-4"
        },
        "template": {
            "metadata": {
                "labels": {
                    "name": "web",
                    "version": "1-4"
                }
            },
            "spec": {
                "containers": [
                    {
                        "image": "gcr.io/golang-game-of-life/web:v1-4",
                        "name": "web",
                        "ports": [
                            {
                                "containerPort": 8080,
                                "hostPort": 8080,
                                "protocol": "TCP"
                            }
                        ]
                    }
                ]
            }
        }
    }
}
